{
    "name": "niyko/rocket",
    "description": "Rocket is a static site generator written in PHP which uses Laravel's Blade template engine under the hood to generate build.",
    "type": "library",
    "version": "1.0.2",
    "license": "MIT",
    "authors": [
        {
            "name": "Niyko",
            "email": "hello@niyko.com"
        }
    ],
    "minimum-stability": "stable",
    "prefer-stable": true,
    "autoload": {
        "psr-4": {
            "Niyko\\Rocket\\": "src/",
            "Niyko\\Rocket\\Console\\": "src/Console/",
            "Niyko\\Rocket\\Console\\Commands\\": "src/Console/Commands/"
        },
        "files": [
            "src/Global.php"
        ]
    },
    "require": {
        "php": ">=8.0.0",
        "symfony/var-dumper": "^6.4",
        "eftec/bladeone": "^4.13",
        "spatie/ignition": "^1.14",
        "symfony/console": "^6.4",
        "symfony/filesystem": "^6.4",
        "nelexa/zip": "^4.0",
        "statix/server": "^0.5.0",
        "nicmart/string-template": "^0.1.3",
        "dfridrich/php-mime-type": "^3.0"
    }
}